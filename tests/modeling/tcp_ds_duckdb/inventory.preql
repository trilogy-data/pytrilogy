import warehouse as warehouse;
import item as item;
import date as date;

property <warehouse.id, date.id>.quantity_on_hand int;

datasource warehouse_inventory (
    inv_warehouse_sk: ?warehouse.id,
    inv_date_sk: date.id,
    inv_quantity_on_hand: quantity_on_hand,
    inv_item_sk:item.id,
    )
grain (warehouse.id, date.id)
address memory.inventory;