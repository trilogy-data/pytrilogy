import std.geography;

key sample_id int;

key species string;
key country string::country;
key region string;
key climate_zone string;
property <sample_id>.mean_annual_temp_C float;
property <sample_id>.mean_annual_precip_mm int;
property <sample_id>.latitude float::latitude;
property <sample_id>.longitude float::longitude;

datasource genus_dim (
    species
)
grain (species)
query '''
select
    'setosa' as species,
    union all
 select   'versicolor' as species,
    union all
 select   'virginica' as species
    union all
 select   'madeupicium' as species
''';

datasource genus_info (
	species,
	country,
	region,
	climate_zone,
	mean_annual_temp_C,
	mean_annual_precip_mm,
	latitude,
	longitude,
	sample_id,
)
grain (sample_id)
address genus_info;